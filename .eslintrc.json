{
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:testing-library/react"
  ],
  "plugins": ["import", "testing-library"],
  "rules": {
    "quotes": ["error", "double"],
    "no-unused-vars": "error",
    "no-undef": "error",
    "import/no-unresolved": "error"
  }
}
